{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "almond",
  "name": "AWFixer Holdings Legal",
  "colors": {
    "primary": "#AD1457",
    "light": "#AD1457",
    "dark": "#AD1457"
  },
  "favicon": {
    "light": "/images/morty.jpeg",
    "dark": "/images/morty.jpeg"
  },
  "navigation": {
    "global": {
      "anchors": [
        {
          "anchor": "Discord",
          "icon": "thumbs-up",
          "color": {
            "light": "#AD1457",
            "dark": "#AD1457"
          },
          "href": "https://discord.awfixer.com"
        }
      ]
    },
    "tabs": [
      {
        "tab": "Legal",
        "groups": [
          {
            "group": "Terms",
            "pages": [
              "legal/terms/use",
              "legal/terms/service"
            ]
          }
        ]
      }
    ]
  },
  "logo": "/images/morty.jpeg",
  "navbar": {
    "links": [
      {
        "label": "Discord",
        "href": "https://discord.awfixer.com"
      }
    ]
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/awandfriends",
      "github": "https://github.com/awfixer-org",
      "discord": "https://discord.awfixer.com"
    },
    "links": [
      {
        "header": "Founders",
        "items": [
          {
            "label": "AWFixer",
            "href": "https://awfixer.me"
          },
          {
            "label": "jinx",
            "href": "https://imjinx.lol"
          }
        ]
      }
    ]
  },
  "description": "Legal Notes and Notices",
  "background": {
    "decoration": "grid"
  },
  "banner": {
    "dismissible": true,
    "content": "Join our Discord!"
  },
  "interaction": {
    "drilldown": true
  },
  "metadata": {
    "timestamp": true
  },
  "seo": {
    "indexing": "navigable"
  },
  "styling": {
    "latex": true,
    "codeblocks": "dark"
  },
  "icons": {
    "library": "fontawesome"
  }
}
